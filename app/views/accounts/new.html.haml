- form_for @rubyist, :url => account_path do |form|
  != form.error_messages

  %dl
    %dt!= form.label :username
    %dd!= form.text_field :username

    %dt!= form.label :full_name
    %dd!= form.text_field :full_name

    %dt!= form.label :email
    %dd!= form.text_field :email

  != form.submit 'Sign up'
